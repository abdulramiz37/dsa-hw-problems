const arr = [8,4,1,5,9,2];




const  insertionSort = (arr) => {
  
  
  for (let n1 = 1; n1 < arr.length; n1++){
    
    let temp= arr[n1];
    let n2=n1-1;
    
    while( n2 >=0 && arr[n2] >temp){
 
         arr[n2+1]=arr[n2];
            n2--;
         }
      arr[n2+1]=temp;
    
    
    }
    console.log(arr); 
}

insertionSort(arr);
